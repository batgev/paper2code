# Paper2Code Secrets Template
# Copy this file to secrets.yaml and add your API keys
# IMPORTANT: Never commit this file to version control!

# OpenAI Configuration
openai:
  api_key: "your_openai_api_key_here"
  base_url: "https://api.openai.com/v1"  # Default OpenAI API URL

# Anthropic Configuration
anthropic:
  api_key: "your_anthropic_api_key_here"

# GitHub Configuration (optional, for repository search)
github:
  token: "your_github_token_here"  # Personal access token for higher rate limits

# Search APIs (optional)
brave_search:
  api_key: "your_brave_api_key_here"

# Other optional services
openweather:
  api_key: "your_openweather_api_key_here"

# Local LLMs (Ollama)
ollama:
  base_url: "http://127.0.0.1:11434"